{
	"manifest_version": 2,
	"name": "Wine To Blood Wine",
	"version": "1.1.2",
	"description": "Replaces normal words with their Star Trek counterparts.",
	"options_page": "tests/tests.html",
	"browser_action": {
      "default_title": "Tests",
      "default_popup": "tests/tests.html"
    },
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"js": ["js/content_script.js"],
			"css": ["css/content_styles.css"],
			"run_at": "document_end"
		}
	]
}

